{
    "name": "alf007/topiccalendar",
    "type": "phpbb-extension",
    "description": "Extension to define event date from topics, displayed on a calendar page - conversion/update from MyCalendar",
    "homepage": "https://github.com/Alf007/topiccalendar",
    "version": "1.0.1",
    "time": "2021-11-12",
    "keywords": ["phpbb", "extension", "calendar", "topic", "event", "birthdays"],
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Alf007",
            "role": "Developper"
        },
        {
            "name": "mojavelinux",
            "homepage": "http://www.mojavelinux.com/projects/mycalendar/",
            "role": "Original phpbb 2 developper"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "composer/installers": "~1.0"
        "phpbb/phpbb": ">=3.2.0"
     },
    "require-dev": {
        "phpbb/epv": "dev-master"
    },
    "extra": {
        "display-name": "Topic Calendar",
        "soft-require": {
            "phpbb/phpbb": ">=3.2.0"
        }
    }
}
